#include <stdio.h>
int main (void){
    int a = 1;
    int b = 2;
    int c = 3;
    if (a > b){
        if (a < c){
            printf ("c = %d\n",c);
        }
        else {
            printf("a = %d\n",a);
        }

    }
    else {
        if (b < c){
            printf("c = %d\n",c);
        }
        else {
            printf("b = %d\n",b);
        }
    }
return 0;
}
